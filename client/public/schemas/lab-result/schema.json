{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://clinicaltrial.nthopinion.com/lab-result/schema.json",
    "title": "Lab Result",
    "titlePlural": "Lab Results",
    "description": "A lab result",
    "properties": {
        "value": {
            "title": "Value",
            "type": "number"
        },
        "units": {
            "title": "Units",
            "description": "",
            "type": "string",
            "enum": ["mg", "ml"]
        },
        "datetime": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "value", "units", "datetime"
    ]
  }
